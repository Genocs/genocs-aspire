{
  "app": {
    "name": "SignalR Service",
    "service": "signalr-service",
    "instance": "000001",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "signalr-service",
    "address": "localhost",
    "port": "5007",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "elk": {
    "enabled": false,
    "url": "http://localhost:9200",
    "indexFormat": "signalr-service-{0:yyyy.MM.dd}",
    "basicAuthEnabled": false,
    "username": "user",
    "password": "secret"
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "signalr-service",
    "requestRetries": 3
  },
  "httpClient": {
    "type": "",
    "retries": 2,
    "services": {
      "pricing": "localhost:5003"
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "signalR",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [
      "/ping",
      "/metrics"
    ]
  },
  "jwt": {
    "certificate": {
      "location": "certs/localhost.cer"
    },
    "secretKey": "JLBMU2VbJZmt42sUwByUpJJF6Y5mG2gPNU9sQFUpJFcGFJdyKxskR3bxh527kax2UcXHvB",
    "expiryMinutes": 30,
    "issuer": "genocs-identity-service",
    "validIssuer": "genocs-identity-service",
    "validateAudience": false,
    "validateIssuer": true,
    "validateLifetime": true,
    "expiry": "01:00:00"
  },
  "logger": {
    "level": "information",
    "applicationName": "signalr-service",
    "excludePaths": [
      "/ping",
      "/metrics"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": false,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5
  },
  "mongodb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "orders-service",
    "seed": false,
    "enableTracing": true
  },
  "prometheus": {
    "enabled": false
  },
  "rabbitMq": {
    "namespace": "signalr",
    "connectionName": "signalr-service",
    "messagesPersisted": true,
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true,
      "logConnectionStatus": true,
      "logMessagePayload": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost",
      "rabbitmq"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "signalr"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    },
    "maxProducerChannels": 1000
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "signalr:"
  },
  "restEase": {
    "loadBalancer": "",
    "services": []
  },
  "security": {
    "certificate": {
      "enabled": true,
      "header": "Certificate",
      "allowedHosts": [
        "localhost"
      ]
    }
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "SignalR Service",
    "version": "v1",
    "routePrefix": "swagger"
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret",
    "kv": {
      "enabled": false,
      "engineVersion": 2,
      "mountPoint": "kv",
      "path": "signalr-service/settings"
    },
    "pki": {
      "enabled": false,
      "roleName": "signalr-service",
      "commonName": "signalr-service.convey.io"
    },
    "lease": {
      "mongo": {
        "type": "database",
        "roleName": "signalr-service",
        "enabled": false,
        "autoRenewal": true,
        "templates": {
          "connectionString": "mongodb://{{username}}:{{password}}@localhost:27017"
        }
      }
    }
  },
  "webApi": {
    "bindRequestFromRoute": true
  }
}